#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib64/ivl/system.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib64/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib64/ivl/v2005_math.vpi";
:vpi_module "/usr/lib64/ivl/va_math.vpi";
S_0x56283a1d5490 .scope module, "testbench_sumador4bits" "testbench_sumador4bits" 2 4;
 .timescale -9 -12;
v0x56283a223420_0 .var "a", 3 0;
v0x56283a2234e0_0 .var "b", 3 0;
v0x56283a223580_0 .net "sum", 4 0, L_0x56283a223900;  1 drivers
S_0x56283a1d5620 .scope module, "uut" "sumador4bits" 2 11, 3 2 0, S_0x56283a1d5490;
 .timescale -9 -12;
    .port_info 0 /INPUT 4 "a";
    .port_info 1 /INPUT 4 "b";
    .port_info 2 /OUTPUT 5 "sum";
v0x56283a1d5850_0 .net *"_ivl_0", 4 0, L_0x56283a223650;  1 drivers
L_0x7fa0a6da8018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x56283a222e30_0 .net *"_ivl_3", 0 0, L_0x7fa0a6da8018;  1 drivers
v0x56283a222f10_0 .net *"_ivl_4", 4 0, L_0x56283a2237c0;  1 drivers
L_0x7fa0a6da8060 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x56283a222fd0_0 .net *"_ivl_7", 0 0, L_0x7fa0a6da8060;  1 drivers
v0x56283a2230b0_0 .net "a", 3 0, v0x56283a223420_0;  1 drivers
v0x56283a2231e0_0 .net "b", 3 0, v0x56283a2234e0_0;  1 drivers
v0x56283a2232c0_0 .net "sum", 4 0, L_0x56283a223900;  alias, 1 drivers
L_0x56283a223650 .concat [ 4 1 0 0], v0x56283a223420_0, L_0x7fa0a6da8018;
L_0x56283a2237c0 .concat [ 4 1 0 0], v0x56283a2234e0_0, L_0x7fa0a6da8060;
L_0x56283a223900 .arith/sum 5, L_0x56283a223650, L_0x56283a2237c0;
    .scope S_0x56283a1d5490;
T_0 ;
    %pushi/vec4 2, 0, 4;
    %store/vec4 v0x56283a223420_0, 0, 4;
    %pushi/vec4 3, 0, 4;
    %store/vec4 v0x56283a2234e0_0, 0, 4;
    %delay 10000, 0;
    %load/vec4 v0x56283a223580_0;
    %cmpi/ne 9, 0, 5;
    %jmp/0xz  T_0.0, 6;
    %vpi_call 2 24 "$display", "Error en la Prueba 1" {0 0 0};
    %vpi_call 2 25 "$fatal" {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call 2 27 "$display", "Prueba 1 pasada correctamente" {0 0 0};
T_0.1 ;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x56283a223420_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x56283a2234e0_0, 0, 4;
    %delay 10000, 0;
    %load/vec4 v0x56283a223580_0;
    %cmpi/ne 22, 0, 5;
    %jmp/0xz  T_0.2, 6;
    %vpi_call 2 35 "$display", "Error en la Prueba 2" {0 0 0};
    %vpi_call 2 36 "$fatal" {0 0 0};
    %jmp T_0.3;
T_0.2 ;
    %vpi_call 2 38 "$display", "Prueba 2 pasada correctamente" {0 0 0};
T_0.3 ;
    %vpi_call 2 43 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
